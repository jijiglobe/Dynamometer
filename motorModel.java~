import java.util.*;
import java.io.*;

public class motorModel{
private class motor{
    private float stall, free, torquePerSpeed, speedPerTorque;
    public motor(float stallTorque, float freeSpeed){
	//torque should be in N * M
	//free speed should be in RPM
	//system will automatically convert to rad/s
	stall = stallTorque;
	free = freeSpeed;
	torquePerSpeed = stall/free;
	speedPerTorque = free/stall;
    }

    public float getSpeed(float torque){
	return torque * speedPerTorque;
    }

    public float getTorque(float speed){
	return speed * torquePerSpeed;
    }
}

private class inertialDisk{
    private float inertia;
    private float velocity;
    private float angle;
    public inertialDisk(float rotationalInertia){
	inertia = rotationalInertia;
	angle = 0.0;
    }

    public float accelerate(float torque,float time){
	float acceleration = torque / inertia;
	float newVelocity = velocity + (acceleration * time);
	float averageVelocity = (newVelocity + velocity)/2;
	angle += averageVelocity * time;
	return angle;
    }
    public float getVelocity(){
	return velocity;
    }
    public float getAngle(){
	return angle;
    }
}

private class spring{
    private float springConstant;

    public spring(float constant){
	constant = springConstant;
    }
    public float getTorque(float angle){
	return springConstant * angle;
    }
}

    public static void main(String[] args){
	
    }
}
